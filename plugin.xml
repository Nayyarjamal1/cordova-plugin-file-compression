<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-file-compression" version="1.0.0">
   <name>CompressFile</name>
   <js-module name="CompressFile" src="www/CompressFile.js">
      <clobbers target="cordova.plugins.CompressFile" />
   </js-module>
    <platform name="ios">

        <config-file parent="/*" target="config.xml">
            <feature name="CompressFile">
                <param name="ios-package" value="CompressFile" />
            </feature>
        </config-file>

        <source-file src="src/ios/CompressFile.swift" />
        
    </platform>
   <platform name="android">
      <config-file parent="/*" target="res/xml/config.xml">
         <feature name="CompressFile">
            <param name="android-package" value="cordova-plugin-file-compression.CompressFile" />
         </feature>
      </config-file>
      <config-file parent="/*" target="AndroidManifest.xml" />
      <source-file src="src/android/CompressFile.java" target-dir="src/cordova-plugin-file-compression/CompressFile" />
   </platform>
</plugin>
